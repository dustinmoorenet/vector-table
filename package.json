{
  "name": "vector-table",
  "version": "0.0.0",
  "description": "Vector display and manipulation element",
  "repository": "https://github.com/dustinmoorenet/vector-table",
  "main": "index.js",
  "scripts": {
    "test": "./scripts/test.sh",
    "build:css": "node-sass ./client/sass/index.scss ./client/css/index.css",
    "watch:css": "node-sass --watch --source-map=true ./client/sass/index.scss ./client/css/index.css",
    "build:js": "browserify ./client/js/index.js --outfile ./client/app.bundle.js",
    "watch:js": "watchify ./client/js/index.js --verbose --debug --outfile ./client/app.bundle.js",
    "build": "npm run build:css && npm run build:js",
    "watch": "parallelshell 'npm run watch:css' 'npm run watch:js'"
  },
  "author": "Dustin Moore",
  "license": "MIT",
  "devDependencies": {
    "ampersand-events": "1.1.1",
    "babelify": "6.1.2",
    "brfs": "1.4.0",
    "browserify": "10.2.3",
    "events-mixin": "1.2.0",
    "expect": "1.6.0",
    "json-query": "1.5.0",
    "karma": "0.12.35",
    "karma-browserify": "4.2.1",
    "karma-chrome-launcher": "^0.1.12",
    "karma-cli": "0.0.4",
    "karma-mocha": "0.1.10",
    "lodash": "3.9.3",
    "mocha": "2.2.5",
    "node-sass": "3.1.2",
    "node-uuid": "1.4.3",
    "parallelshell": "1.1.1",
    "request": "2.57.0",
    "request-promise": "0.4.2",
    "svg.js": "git@github.com:wout/svg.js.git",
    "watchify": "3.2.1",
    "when": "3.7.3",
    "workerify": "0.3.0"
  },
  "dependencies": {
    "body-parser": "1.12.4",
    "express": "4.12.4"
  },
  "browserify": {
    "transform": [
      "babelify",
      "brfs",
      "workerify"
    ]
  }
}
