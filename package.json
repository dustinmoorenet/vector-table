{
  "name": "vector-table",
  "version": "0.0.0",
  "description": "Vector display and manipulation element",
  "repository": "https://github.com/dustinmoorenet/vector-table",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "node-sass ./client/sass/index.scss ./client/css/index.css",
    "watch:css": "node-sass --watch --source-map=true ./client/sass/index.scss ./client/css/index.css",
    "build:js": "browserify ./client/js/index.js --outfile ./client/app.bundle.js",
    "watch:js": "watchify ./client/js/index.js --verbose --debug --outfile ./client/app.bundle.js"
  },
  "author": "Dustin Moore",
  "license": "MIT",
  "devDependencies": {
    "ampersand-events": "1.0.1",
    "ampersand-view": "7.2.0",
    "brfs": "1.4.0",
    "browserify": "9.0.3",
    "expect": "1.6.0",
    "gulp": "3.8.11",
    "gulp-browserify": "0.5.1",
    "gulp-consolidate": "0.1.2",
    "gulp-iconfont": "1.0.0",
    "gulp-rename": "1.2.0",
    "gulp-sass": "1.3.3",
    "gulp-util": "3.0.4",
    "json-query": "1.4.0",
    "lodash": "3.5.0",
    "mocha": "2.2.1",
    "node-sass": "3.0.0",
    "node-uuid": "1.4.3",
    "request": "2.55.0",
    "request-promise": "0.4.1",
    "vinyl-source-stream": "1.1.0",
    "watchify": "2.4.0",
    "when": "3.7.2",
    "workerify": "0.3.0"
  },
  "dependencies": {
    "body-parser": "1.12.2",
    "express": "4.12.3"
  },
  "browserify": {
    "transform": [
      "brfs",
      "workerify"
    ]
  }
}
